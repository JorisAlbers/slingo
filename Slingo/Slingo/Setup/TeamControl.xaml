<reactiveUi:ReactiveUserControl 
    x:TypeArguments="local:TeamViewModel" x:Class="Slingo.Setup.TeamControl"
                           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                           xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                           xmlns:local="clr-namespace:Slingo.Setup"
                           xmlns:reactiveUi="http://reactiveui.net"
                           mc:Ignorable="d" 
                           d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <TextBlock x:Name="NameTextBlock"
                   DockPanel.Dock="Top"/>
        <UniformGrid Columns="2" DockPanel.Dock="Top">
            <TextBlock Text="Player 1"/>
            <TextBox x:Name="Player1TextBox"/>
            <TextBlock Text="Player 2"/>
            <TextBox x:Name="Player2TextBox"/>
            </UniformGrid>
        
    </DockPanel>

   
</reactiveUi:ReactiveUserControl>
